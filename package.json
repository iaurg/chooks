{
  "name": "chooks",
  "version": "1.0.0",
  "description": "A plataform to share opinions about books",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "test": "jest",
    "build": "webpack --mode production"
  },
  "jest": {
